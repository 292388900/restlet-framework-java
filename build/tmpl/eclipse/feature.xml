<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="restlet.${edition.id}"
      label="Restlet edition for the JSE platform"
      version="${ant["version-manifest"]}"
      provider-name="Restlet S.A.S.">

   <description url="http://www.example.com/description">
      [Enter Feature Description here.]
   </description>

   <copyright url="http://www.example.com/copyright">
      [Enter Copyright Description here.]
   </copyright>

   <license url="http://www.example.com/license">
      [Enter License Description here.]
   </license>

<#list edition.modules as module>
   <plugin
         id="${module.package}"
         download-size="0"
         install-size="0"
         version="${ant["version-manifest"]}"
         unpack="false"/>
</#list>
<#list edition.libraries as library>
	<#list library.packages as package>
   <plugin
         id="${package.name}"
         download-size="0"
         install-size="0"
         version="${library.versionFull}"
         unpack="false"/>
	</#list>
</#list>

</feature>
