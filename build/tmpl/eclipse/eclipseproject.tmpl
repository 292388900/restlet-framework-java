<#list project.libraries?values as library>
<@pp.nestOutputFile name="${library.rootDirectory}.bnd">
-nouses: true
-noextraheaders: true
Export-Package: *
Bundle-Name: ${library.name!"pouet"}
Bundle-SymbolicName: ${library.manifestId}
